<%- include('./partials/html-head') %>
<%- include('./partials/nav') %>

<main>
  <h1>Welcome</h1>
  <% if (user?.profile.avatar) { %>
    <div>
      <img
        src="<%= user.profile.avatar %>"
        alt="<%= user.profile.name %> avatar"
      />
    </div>
    <%= user.profile.name %> 
  <% } %>
  <% if (!user?.profile.avatar) { %> 
    <h2>Please Login Above</h2>
    <% } %>
</main>

<%- include('./partials/footer') %>
